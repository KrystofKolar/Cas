<Window x:Class="TestFrame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestFrame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FFE0EA0A">
    <Grid>
        <StackPanel HorizontalAlignment="Stretch" Orientation="Vertical" Margin="0,0,0,0">
            <Button MinWidth="100" Background="#FFE6A210" Foreground="White" HorizontalAlignment="Left" Click="Button_Click">Page1</Button>
            <Button MinWidth="100" Content="Page2" Background="#FFD39719" Foreground="White" HorizontalAlignment="Left" Click="Button_Click_1"/>
            <Button>
                <TextBlock FontSize="48">&#x04A8;</TextBlock>
            </Button>
        </StackPanel>
        <Frame x:Name="MainFrame" Margin="0,100,0,0" Background="LightCoral" NavigationUIVisibility="Hidden"/>


    </Grid>
</Window>
